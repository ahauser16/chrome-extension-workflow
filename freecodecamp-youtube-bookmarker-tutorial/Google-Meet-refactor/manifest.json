{
  "name": "Refactor Test",
  "version": "0.1.0",
  "description": "Google Meet refactor of YT bookmark tutorial video",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://meet.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/bookmark.png",
        "assets/play.png",
        "assets/delete.png",
        "assets/save.png"
      ],
      "matches": [
        "https://meet.google.com/*"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/NYS-flag-icon-16.png",
      "32": "assets/NYS-flag-icon-32.png",
      "48": "assets/NYS-flag-icon-48.png",
      "128": "assets/NYS-flag-icon-128.png"
    },
    "default_title": "Notary Log:",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}