  {
    "manifest_version": 3,
    "name": "Google Meet Extension - Long-lived connection test",
    "version": "1.0",
    "description": "A test to see if a long-lived connection is required when developing an extension for Google Meet",
    "icons": {
      "16": "assets/images/NYS-flag-icon-16.png",
      "32": "assets/images/NYS-flag-icon-32.png",
      "48": "assets/images/NYS-flag-icon-48.png",
      "128": "assets/images/NYS-flag-icon-128.png"
    },
    "permissions": [
      "tabs",
      "storage",
      "activeTab",
      "sidePanel"
    ],
    "background": {
      "service_worker": "serviceWorker.js"
    },
    "content_scripts": [
      {
        "matches": ["https://meet.google.com/*"],
        "js": ["contentScript.js"],
        "css": ["contentScript.css"]
      }
    ],"options_ui": {
      "page": "/pages/settings/settings.html"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Click for more options",
      "default_icon": {
        "16": "assets/images/NYS-flag-icon-16.png",
        "32": "assets/images/NYS-flag-icon-32.png",
        "48": "assets/images/NYS-flag-icon-48.png",
        "128": "assets/images/NYS-flag-icon-128.png"
      }
    },
    "side_panel": {
      "default_path": "sidepanel.html"
    }
  }