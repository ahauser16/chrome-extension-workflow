{
  "manifest_version": 3,
  "name": "copy copy copy paste paste paste",
  "version": "1.0",
  "description": "This extension is an attempt to improve on the copy&paste workflow.",
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "sidePanel",
    "offscreen",
    "clipboardWrite",
    "clipboardRead"
  ]
}